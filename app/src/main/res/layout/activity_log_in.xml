<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:id="@+id/main_background_Layout"
    tools:context=".LogIn_Activity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#ffffff"
        android:elevation="3dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="1"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1"
        app:layout_constraintHorizontal_bias="0.5">

        <TextView
            android:id="@+id/logo"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            app:layout_constraintDimensionRatio="w,322:153"
            android:background="@drawable/ic_saeryen_logo_3065f3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHeight_percent="0.05"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.15"
            app:layout_constraintHorizontal_bias="0.5"></TextView>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/biznum_input_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="center"
            android:elevation="0.5dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:background="@drawable/round_popup_bg"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.8"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.065"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.35">



            <EditText
                android:id="@+id/biznum_input"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent="1"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="1"
                android:layout_weight="1"
                android:paddingHorizontal="20dp"
                android:hint="휴대폰 번호 ( - 제외 )"
                android:fontFamily="@font/nanumsquare_acr"
                app:autoSizeTextType="uniform"
                app:autoSizeMinTextSize="0.1sp"
                app:autoSizeStepGranularity="0.1sp"
                android:textColorHint="#C6C6C6"
                android:gravity="center|left"
                android:inputType="number"
                android:textColor="#5F5F5F"
                android:background="@drawable/selector_round_white_bg_focus_selyenblue"
                android:cursorVisible="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5">
            </EditText>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/pw_input_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="center"
            android:elevation="0.5dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:background="@drawable/round_popup_bg"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.8"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.065"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.45">


            <EditText
                android:id="@+id/pw_input"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent="1"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="1"
                android:layout_weight="1"
                android:paddingHorizontal="20dp"
                android:hint="비밀번호"
                android:fontFamily="@font/nanumsquare_acr"
                app:autoSizeTextType="uniform"
                app:autoSizeMinTextSize="0.1sp"
                app:autoSizeStepGranularity="0.1sp"
                android:imeOptions="actionDone"
                android:textColorHint="#C6C6C6"
                android:gravity="center|left"
                android:textColor="#5F5F5F"
                android:inputType="textPassword"
                android:background="@drawable/selector_round_white_bg_focus_selyenblue"
                android:cursorVisible="false"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5">
            </EditText>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <CheckBox
            android:id="@+id/checkbox"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="저장 / 자동 로그인"
            android:fontFamily="@font/nanumsquare_acr"
            android:textSize="12sp"
            android:buttonTint="#777777"
            android:textStyle="bold"
            android:textColor="#777777"
            android:visibility="gone"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.5"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.03"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pw_input_layout"
            app:layout_constraintVertical_bias="0.05"
            app:layout_constraintHorizontal_bias="0.15">
        </CheckBox>

        <TextView
            android:id="@+id/btn_confirm"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/ripple_custom_login_btn"
            android:text="로그인"
            android:textStyle="bold"
            android:padding="12dp"
            android:fontFamily="@font/nanumsquare_acr"
            android:gravity="center"
            android:textColor="#ffffff"
            app:autoSizeTextType="uniform"
            app:autoSizeMinTextSize="1sp"
            app:autoSizeMaxTextSize="25sp"
            app:autoSizeStepGranularity="1sp"
            android:clickable="true"
            android:onClick="onClick_confirm"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.625"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.068"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.9"
            app:layout_constraintHorizontal_bias="0.5"/>

        <TextView
            android:id="@+id/signup_title_text"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:text="또는"
            android:fontFamily="@font/nanumsquare_acr"
            android:gravity="center"
            android:textColor="#5F5F5F"
            app:autoSizeTextType="uniform"
            app:autoSizeMinTextSize="2sp"
            app:autoSizeStepGranularity="1sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHeight_percent="0.023"
            app:layout_constraintWidth_percent="0.2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.6"
            app:layout_constraintHorizontal_bias="0.5"></TextView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/signup_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/round_popup_bg"
            android:gravity="center"
            android:elevation="0.5dp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.8"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.065"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/signup_title_text"
            app:layout_constraintVertical_bias="0.1"
            app:layout_constraintHorizontal_bias="0.5">

            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:background="#C6C6C6"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent="0.5"
                app:layout_constraintWidth_percent="0.004"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5"
                app:layout_constraintHorizontal_bias="0.5"></TextView>

            <TextView
                android:id="@+id/search_pw_text"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:text="비밀번호 찾기"
                android:fontFamily="@font/nanumsquare_acr"
                android:gravity="center"
                android:textColor="#5F5F5F"
                app:autoSizeTextType="uniform"
                app:autoSizeMinTextSize="2sp"
                app:autoSizeStepGranularity="1sp"
                android:clickable="true"
                android:onClick="onClick_find_pw"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent="0.3"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5"
                app:layout_constraintHorizontal_bias="0"></TextView>


            <TextView
                android:id="@+id/signup_text"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="회원가입"
                android:fontFamily="@font/nanumsquare_acr"
                android:gravity="center"
                android:textColor="#5F5F5F"
                android:clickable="true"
                android:onClick="onClick_signup"
                app:autoSizeTextType="uniform"
                app:autoSizeMinTextSize="1sp"
                app:autoSizeMaxTextSize="25sp"
                app:autoSizeStepGranularity="1sp"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5"
                app:layout_constraintHorizontal_bias="1"></TextView>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>